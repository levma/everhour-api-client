{
  "name": "@levma/everhour-api-client",
  "version": "0.1.7",
  "license": "MIT-0",
  "exports": "./main.ts",
  "tasks": {
    "dev": "deno run --watch main.ts",
    "test": "deno test --allow-env --allow-net --env=.env",
    "build_npm": "deno run -A build_npm.ts"
  },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.41.3",
    "@std/assert": "jsr:@std/assert@1"
  },
  "publish": {
    "exclude": [
      ".env",
      ".github/",
      ".vscode/",
      "*_test.ts",
      "build_npm.ts",
      "npm/"
    ]
  }
}
